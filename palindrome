#include<stdio.h>
#include<conio.h>
int main()
{
    int a,b=0,c;
    printf("Enter Number=");
    scanf("%d",&a);
    c=a;

    while(c!=0)
    {
        b=b*10;
        b=b+c%10;
        c=c/10;
    }

    if(a==b)
    printf("%d is a palindrome number",a);
    else
    printf("%d is not a palindrome number",a);

    getch();
    return 0;
}
